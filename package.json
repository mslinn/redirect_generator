{
  "activationEvents": [
    "onCommand:redirect-generator.redirectHttp",
		"onLanguage:html",
		"*"
  ],
  "categories": [
    "Other"
  ],
  "contributes": {
    "commands": [
      {
        "command": "redirect-generator.redirectHttp",
        "title": "Adding a HTTP Redirect"
      }
    ],
		"keybindings": [
      {
        "command": "redirect-generator.redirectHttp",
        "key": "alt+shift+\\",
        "when": "activeEditor"
      }
    ]
  },
	"dependencies": {
  },
  "description": "Injects the URL of a redirect page into Jekyll front matter",
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.64.0",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "@vscode/test-electron": "^2.1.2",
		"eslint": "^8.9.0",
    "glob": "^7.2.0",
    "mocha": "^9.2.1",
    "typescript": "^4.5.5"
  },
  "displayName": "redirect_generator",
  "engines": {
    "vscode": "^1.64.0"
  },
	"license": "A2",
  "main": "./out/extension.js",
  "name": "redirect-generator",
	"repository": {
    "type": "git",
    "url": "https://github.com/mslinn/redirect_generator"
  },
  "scripts": {
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js",
    "vscode:prepublish": "npm run compile",
    "watch": "tsc -watch -p ./"
  },
  "version": "0.0.1"
}
